package make;

/** An exception in our makefile.
 *  @author Andrew Berger*/
class MakeFormatException extends RuntimeException {

    /** MakeformateException with no message.*/
    MakeFormatException() {
    }

    /** MakeFormatExceptino with MSG at message.*/
    MakeFormatException(String msg) {
        super(msg);
    }

}
